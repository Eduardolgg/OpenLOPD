    <div class="formEntry"><div class="formDesc"><span class="formText"><fmt:message bundle="${StandardBundle}" key="label.nombre" />: (*)</span></div><div class="formData"><input id="perNombre" class="formBox" type="text" name="perNombre" value="" maxlength="50" /></div></div>
    <div class="formEntry"><div class="formDesc"><span class="formText"><fmt:message bundle="${StandardBundle}" key="label.apellido1" />: (*)</span></div><div class="formData"><input id="perApellido1" class="formBox" type="text" name="perApellido1" value="" maxlength="50" /></div></div>
